		<variable includeBareGround="false">
			<standardName>Net Shortwave Surface Radiation</standardName>
			<longName>Net downward shortwave radiation at the surface</longName>
			<shortName>rss</shortName>
			<units>W/$m^2$</units>
			<variants>
				<variant>
					<nameInCode>fsstar_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>fsstar_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Net longwave surface radiation</standardName>
			<longName>Net longwave radiation at the surface</longName>
			<shortName>rls</shortName>
			<units>W/$m^2$</units>
			<variants>
				<variant>
					<nameInCode>flstar_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>flstar_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Surface Net Sensible Heat Flux</standardName>
			<longName>Surface Net Sensible Heat Flux</longName>
			<shortName>hfss</shortName>
