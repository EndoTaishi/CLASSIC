			</longName>
			<shortName>ts</shortName>
			<units>$\circ$C</units>
			<variants>
				<variant>
					<nameInCode>taacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Surface Albedo</standardName>
			<longName>
				Grid cell average albedo for all wavelengths.
			</longName>
			<shortName>albs</shortName>
