			<variants>
				<variant>
					<nameInCode>qh_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>qh_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Surface Net Latent Heat Flux</standardName>
			<longName>Surface Net Latent Heat Flux</longName>
			<shortName>hfls</shortName>
			<units>W/$m^2$</units>
			<variants>
				<variant>
					<nameInCode>qe_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>qe_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Surface Snow Amount</standardName>
			<longName>
				The mass of surface snow on the land portion of the grid
				cell divided by the land area in the grid cell reported as missing
				where the land fraction is 0 excludes snow on vegetation canopy or
				on sea ice.
			</longName>
			<shortName>snw</shortName>
			<units>kg/$m^2$</units>
			<variants>
				<variant>
					<nameInCode>snoacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Liquid water content of snow pack</standardName>
			<longName>
				The total mass of liquid water contained interstitially
				within the whole depth of the snow pack of the land portion of a
				grid cell divided by the area of the land portion of the cell.
			</longName>
			<shortName>wsnw</shortName>
			<units>kg/$m^2$</units>
			<variants>
				<variant>
					<nameInCode>wsnoacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Total Run-off</standardName>
			<longName>
				The total run-off (including drainage through the base of
				the soil model) per unit area leaving the land portion of the grid
				cell.
			</longName>
			<shortName>mrro</shortName>
			<units>mm/year</units>
			<variants>
				<variant>
					<nameInCode>rofacc_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>rofacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Precipitation</standardName>
			<longName>
				Preciptiation, includes both solid and liquid phases
			</longName>
			<shortName>pr</shortName>
