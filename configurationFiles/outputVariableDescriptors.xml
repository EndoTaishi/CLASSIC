				<variant>
					<nameInCode>preacc_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>preacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Evaporation</standardName>
			<longName>
				Evaporation at surface: flux of water into the atmosphere
				due to conversion of both liquid and solid phases to vapor (from
				underlying surface and vegetation)
			</longName>
			<shortName>evspsbl</shortName>
			<units>mm/year</units>
			<variants>
				<variant>
					<nameInCode>evapacc_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>evapacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Water Evaporation from Soil</standardName>
			<longName>
				Water evaporation from soil (including sublimation)
			</longName>
			<shortName>evspsblsoi</shortName>
			<units>kg/$m^2$</units>
			<variants>
				<variant>
					<nameInCode>groundevap</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Water Evaporation from Canopy</standardName>
			<longName>
				The canopy evaporation and sublimation; may include dew
				formation as a negative flux.
			</longName>
			<shortName>evspsblveg</shortName>
			<units>mm/month</units>
			<variants>
				<variant>
					<nameInCode>canopyevap</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Transpiration</standardName>
