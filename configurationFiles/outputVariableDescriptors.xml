			<shortName>mrsfl</shortName>
			<units>$m^3$/$m^3$</units>
			<variants>
				<variant>
					<nameInCode>thicacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>layer</outputForm>
				</variant>
