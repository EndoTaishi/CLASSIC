				<variant>
					<nameInCode>altotacc_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>altotacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Temperature of Soil</standardName>
			<longName>
				Temperature of each soil layer. Reported as missing for
				grid cells with no land.
			</longName>
			<shortName>tsl</shortName>
			<units>$\circ$C</units>
			<variants>
				<variant>
					<nameInCode>tbaracc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>layer</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Liquid water content of soil layer</standardName>
			<longName>
				In each soil layer, the mass of water in liquid phase.
				Reported as "missing" for grid cells occupied entirely by "sea"
			</longName>
			<shortName>mrsll</shortName>
			<units>$m^3$/$m^3$</units>
			<variants>
				<variant>
					<nameInCode>thlqacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>layer</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Frozen water content of soil layer</standardName>
			<longName>
