			<variants>
				<variant>
					<nameInCode>transpacc_yr</nameInCode>
					<timeFrequency>annually</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
				<variant>
					<nameInCode>transpacc_mo</nameInCode>
					<timeFrequency>monthly</timeFrequency>
					<outputForm>grid</outputForm>
				</variant>
			</variants>
		</variable>

		<variable includeBareGround="false">
			<standardName>Surface Temperature</standardName>
			<longName>
