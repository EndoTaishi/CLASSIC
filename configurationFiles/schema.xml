<element name="variableSet" xmlns="http://relaxng.org/ns/structure/1.0">
	<oneOrMore>
		<element name="group">
			<attribute name="type">
				<text />
			</attribute>
			<attribute name="description">
				<text />
			</attribute>
			<oneOrMore>
				<element name="variable">
					<attribute name="includeBareGround">
						<text />
					</attribute>
					<element name="standardName">
						<text />
					</element>
					<element name="longName">
						<text />
					</element>
					<element name="shortName">
						<text />
					</element>
					<element name="units">
						<text />
					</element>
					<element name="variants">
						<zeroOrMore>
							<element name="variant">
								<element name="nameInCode">
									<text />
								</element>
								<element name="timeFrequency">
									<text />
								</element>
								<element name="outputForm">
									<text />
								</element>
								<optional>
									<element name="units">
										<text />
									</element>
								</optional>
								<optional>
									<element name="comment">
										<text />
									</element>
								</optional>
							</element>
						</zeroOrMore>
					</element>
					<optional>
						<element name="comment">
							<text />
						</element>
					</optional>
				</element>
			</oneOrMore>
		</element>
	</oneOrMore>
</element>